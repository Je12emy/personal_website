---
import BaseHead from '../components/BaseHead.astro';
import Header from '../components/Header.astro';
import Footer from '../components/Footer.astro';
import { SITE_TITLE, SITE_DESCRIPTION } from '../consts';
---

<!doctype html>
<html lang="en">
	<head>
		<BaseHead title={SITE_TITLE} description={SITE_DESCRIPTION} />
	</head>
	<body>
		<Header />
		<main>
			<h1>You made it!</h1>
			<p>
			Welcome to my little corner of the internet. I hope you enjoy your time in here while browsing, if you do, please let me know in any of my social media.
			</p>
		</main>
		<Footer />
	</body>
</html>
